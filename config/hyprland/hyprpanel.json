{
  "bar.customModules.storage.paths": [
    "/"
  ],
  "theme.font.name": "UbuntuMono Nerd Font Mono",
  "theme.font.label": "UbuntuMono Nerd Font Mono",
  "tear": true,
  "menus.power.lowBatteryNotification": true,
  "theme.bar.buttons.modules.cpu.enableBorder": false,
  "menus.dashboard.recording.path": "$HOME/Videos",
  "menus.dashboard.stats.enable_gpu": true,
  "menus.dashboard.shortcuts.enabled": true,
  "bar.workspaces.show_numbered": true,
  "scalingPriority": "hyprland",
  "theme.font.size": "1rem",
  "bar.layouts": {
    "0": {
      "left": [
        "dashboard",
        "workspaces",
        "windowtitle",
        "media"
      ],
      "middle": [
        "clock",
        "notifications"
      ],
      "right": [
        "systray",
        "volume",
        "bluetooth",
        "network",
        "battery"
      ]
    }
  },
  "menus.volume.raiseMaximumVolume": true,
  "menus.clock.time.military": true,
  "menus.clock.time.hideSeconds": true,
  "menus.clock.weather.enabled": false,
  "menus.dashboard.shortcuts.left.shortcut1.command": "firefox -P UFSC",
  "menus.dashboard.shortcuts.left.shortcut1.tooltip": "UFSC",
  "menus.dashboard.shortcuts.left.shortcut4.command": "wofi -GI -y 0 -x 400 --sort-order alphabetical -i --show drun",
  "menus.dashboard.directories.left.directory3.label": "󰚝 Github",
  "menus.dashboard.directories.left.directory3.command": "bash -c \"xdg-open $HOME/Github/\"",
  "theme.bar.buttons.modules.microphone.enableBorder": false,
  "theme.bar.buttons.modules.ram.enableBorder": false,
  "menus.media.noMediaText": "-",
  "menus.media.hideAlbum": false,
  "theme.bar.floating": false,
  "bar.autoHide": "never",
  "theme.bar.outer_spacing": "0",
  "theme.bar.buttons.y_margins": "0.1em",
  "theme.bar.border_radius": "0.4em",
  "theme.bar.menus.monochrome": true,
  "wallpaper.enable": false,
  "menus.dashboard.shortcuts.left.shortcut1.icon": "󰈹",
  "menus.dashboard.controls.enabled": false,
  "menus.dashboard.powermenu.avatar.name": "Maykon",
  "bar.launcher.autoDetectIcon": true,
  "bar.workspaces.show_icons": false,
  "bar.workspaces.showWsIcons": false,
  "bar.workspaces.applicationIconOncePerWorkspace": true,
  "bar.windowtitle.truncation": true,
  "bar.windowtitle.truncation_size": 50,
  "menus.media.hideAuthor": false,
  "theme.bar.buttons.volume.enableBorder": false,
  "theme.bar.buttons.systray.enableBorder": false,
  "bar.clock.format": "%b %a %d  %H:%M",
  "theme.bar.buttons.media.enableBorder": false,
  "bar.media.show_active_only": true,
  "bar.notifications.show_total": true,
  "bar.notifications.hideCountWhenZero": true,
  "theme.bar.buttons.notifications.enableBorder": false
}